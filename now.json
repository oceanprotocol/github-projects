{
  "version": 2,
  "name": "oceanprotocol-github",
  "alias": "oceanprotocol-github.now.sh",
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
